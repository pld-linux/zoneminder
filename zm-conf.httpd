Alias /zm/ "/usr/share/zm/"
<Directory "/usr/share/zm/">
    Options Indexes MultiViews FollowSymLinks
    AllowOverride AuthConfig Limit
    Order allow,deny
    Allow from all
</Directory>

ScriptAlias /zm/cgi-bin/ "/usr/share/zm/cgi-bin/"
<Directory "/usr/share/zm/cgi-bin/">
    AllowOverride AuthConfig Limit
    Options ExecCGI
    Order allow,deny
    Allow from all
</Directory>

